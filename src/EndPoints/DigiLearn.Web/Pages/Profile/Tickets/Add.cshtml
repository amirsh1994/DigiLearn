@page
@model DigiLearn.Web.Pages.Profile.Tickets.AddModel
@{
    ViewData["title"] = " افزودن تیکت جدید";
    Layout = "Shared/_ProfileLayout";
}
                <div class="row ">
                    <div class="col-12 info shadow pl-0 pr-0">
                        <p class="title">
                            <i class="fa fa-address-card-o"></i>
                            @ViewData["title"]
                        </p>
                        <div class="info-body p-2">
                            <form class="row" method="post">
                                <div class="col-12">
                                    @Html.EditorFor(x => x.Title)
                                </div>
                                <div class="col-12">
                                    @Html.EditorFor(x => x.Text)
                                </div>
                                <div class="col-12">
                                    <button class="btn  btn-save  mt-3"> ثیت تیکت</button>

                                </div>
                            </form>
                        </div>
                    </div>
                </div>
